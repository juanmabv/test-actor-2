{
    "actorSpecification": 1,
    "name": "my-actor",
    "title": "Getting started with Python and Selenium",
    "description": "Scrapes titles of websites using Selenium.",
    "version": "0.0",
    "meta": {
         "templateId": "python-selenium"
    },
    "input": "./input_schema.json",
    "dockerfile": "./Dockerfile",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "URLs and their titles",
            "views": {
                "titles": {
                    "title": "URLs and their titles",
                    "transformation": {
                        "fields": [
                            "enlace",
                            "fecha",
                            "titulo"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "nombre": {
                                "label": "Nombre a la oferta",
                                "format": "text"
                            },
                            "id": {
                                "label": "id de la oferta",
                                "format": "text"
                            },
                            "ubicacion": {
                                "label": "Ubicación de la oferta",
                                "format": "text"
                            },
                            "fecha_pub": {
                                "label": "Fecha de publicación",
                                "format": "text"
                            },
                            "vacantes": {
                                "label": "Número de vacantes",
                                "format": "text"
                            },
                            "ett": {
                                "label": "Empresa de trabajo temporal",
                                "format": "text"
                            },
                            "discapacidad": {
                                "label": "Puesto para persona con discapacidad",
                                "format": "text"
                            },
                            "nivel_profesional": {
                                "label": "Nivel profesional",
                                "format": "text"
                            },
                            "experiencia": {
                                "label": "Experiencia requerida",
                                "format": "text"
                            },
                            "nivel_academico": {
                                "label": "nivel académico",
                                "format": "text"
                            },
                            "titulacion": {
                                "label": "Titulación necesaria",
                                "format": "text"
                            },
                            "idiomas": {
                                "label": "Idiomas",
                                "format": "text"
                            },
                            "informática": {
                                "label": "Informática",
                                "format": "text"
                            },
                            "otros_ctos": {
                                "label": "Otros conocimientos necesarios",
                                "format": "text"
                            },
                            "certif_prof": {
                                "label": "Certificados de Profesionalidad",
                                "format": "text"
                            },
                            "permisos_conducir": {
                                "label": "Permisos de conducir",
                                "format": "text"
                            },
                            "info_adicional": {
                                "label": "Información adicional",
                                "format": "text"
                            }
                        }
                    }
                }
            }
        }
    }
}
